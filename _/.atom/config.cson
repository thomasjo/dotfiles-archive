"*":
  core:
    customFileTypes:
      "source.cpp": [
        "h"
      ]
      "source.json": [
        ".eslintrc"
      ]
      "text.tex.latex": [
        "cls"
      ]
    disabledPackages: [
      "background-tips"
      "feedback"
      "metrics"
      "welcome"
    ]
    projectHome: "/Users/thomasjo/Code"
    telemetryConsent: "limited"
  editor:
    fontSize: 16
    invisibles:
      tab: "â‡¥"
    lineHeight: 1.5
    scrollPastEnd: false
    softWrapAtPreferredLineLength: true
    softWrapHangingIndent: 2
  "exception-reporting":
    userId: "31389ec7-b10c-86d6-a4a9-1bb6d981c30d"
  latex:
    enableShellEscape: true
    engine: "xelatex"
    loggingLevel: "error"
    outputDirectory: ".output"
  "markdown-preview":
    useGitHubStyle: true
  "one-dark-ui":
    fontSize: 11
    layoutMode: "Spacious"
    tabSizing: "Even"
  "one-light-ui":
    fontSize: 11
    layoutMode: "Spacious"
    tabSizing: "Even"
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
    squashDirectoryNames: true
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
".c.source":
  editor:
    softWrapHangingIndent: 2
    tabLength: 2
".cpp.source":
